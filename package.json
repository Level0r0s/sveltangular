    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "sirv-cli": "^0.4.4",
    "svelte-preprocess": "^3.1.2",
    "terser-folder": "^2.0.0",
