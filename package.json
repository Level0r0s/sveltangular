    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/preset-env": "^7.6.3",
    "@pyoner/svelte-types": "^3.4.4-2",
    "@types/node": "^12.11.2",
    "autoprefixer": "^9.7.0",
    "babel-jest": "^24.9.0",
    "cssnano": "^4.1.10",
    "gzipper": "^3.3.0",
    "jest": "^24.9.0",
    "jest-transform-svelte": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.21",
    "postcss-import": "^12.0.1",
