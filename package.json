{
  "name": "sveltangular",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "rebabel": "babel public/system --out-dir public/system",
    "minify": "terser-folder --config-file 'terser.config.json' public/ -x .js -e -o public",
    "gzip": "gzipper public",
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "prebuild": "rimraf public",
    "dev": "run-p prebuild start:dev autobuild",
    "build:prod": "NODE_ENV=production run-s build rebabel minify gzip",
    "start:dev": "sirv public --dev",
    "start": "sirv public",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
